

<h4>Incomplete Translations</h4>
<div class="table-wrapper">
	<table>
		<thead>
			<tr>
				<th>Subject</th>
				<th>From:</th>
				<th>To: </th>
                <th>Price: </th>
				<th>Apply: </th>
			</tr>
		</thead>
		<tbody>
			<% jobs.each do |job| %>
			<tr>
				<td><%= job.subject %> </td>
				<td><%= job.language_to.en %></td>
				<td><%= job.language_to.en %></td>
				<td><%= job.price %></td>	
				<td><%= job.stripe_charge_id %></td>	
			</tr>
			<% end %>
		</tbody>
</table>
</div>



